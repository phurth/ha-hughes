{
  "domain": "hughes",
  "name": "Hughes Power Watchdog",
  "codeowners": [
    "@petehurth"
  ],
  "config_flow": true,
  "dependencies": [
    "bluetooth_adapters"
  ],
  "documentation": "https://github.com/phurth/ha-hughes",
  "bluetooth": [
    {
      "local_name": "PMD*"
    },
    {
      "local_name": "PWS*"
    },
    {
      "local_name": "WD_*"
    }
  ],
  "iot_class": "local_push",
  "requirements": [
    "bleak>=0.21.0",
    "bleak-retry-connector>=3.1.0"
  ],
  "version": "1.0.1"
}
